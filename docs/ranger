:rename new_file_name.txt      # Renames the selected file.
:bulkrename                    # Bulk renames selected files.
:search file_name              # Searches for a file.
:set show_hidden!              # Toggle hidden files.

# History
H                              # History move back.
L                              # History move forward.

# Tabs
Ctrl-N                         # New tab.
Alt+Left / Tab                 # Move to next tab.
Alt+Right / Shift+Tab          # Move to previous tab.
Alt+N                          # Move to Nth tab.
gc                             # Close tab.

# Selection
Space                          # Select file.
v                              # Select all files.
uv                             # Deselect all files.

# Clipboard
yy                             # Copy selection.
dd                             # Cut selection.
pp                             # Paste selection.

# Bookmarks
mX                             # Create bookmark with keybind X.
`X                             # Move to bookmark X.
